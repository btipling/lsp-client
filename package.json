{
  "name": "lsp-client",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "lint": "yarn tslint -- --fix -c ./tslint.json './src/**/*'",
    "build-main": "yarn tsc -- --sourceMap --outFile ./main.js ./src/main.ts",
    "build-index": "yarn tsc -- -p tsconfig.json",
    "run-electron": "yarn electron .",
    "debug": "yarn build-index",
    "build": "yarn lint && yarn build-main && yarn debug",
    "start": "yarn build && yarn debug"
  },
  "dependencies": {
    "@cycle/base": "^4.3.0",
    "@cycle/dom": "^14.3.0",
    "@cycle/rxjs-run": "^3.3.0",
    "@cycle/xstream-run": "^4.2.0",
    "@types/core-js": "^0.9.35",
    "@types/electron": "^1.4.32",
    "@types/ramda": "^0.0.3",
    "electron": "^1.4.15",
    "electron-default-menu": "^1.0.0",
    "ramda": "^0.23.0",
    "rxjs": "^5.1.1",
    "tachyons": "^4.6.2",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6",
    "xstream": "^10.2.0"
  }
}
