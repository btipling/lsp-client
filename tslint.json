{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules"
  ],
  "rules": {
    "object-curly-spacing": [ true, "always" ],
    "array-bracket-spacing": [ true, "always" ],
    "curly": true,
    "quotemark": [ true, "single" ],
    "object-literal-sort-keys": false,
    "no-empty": false,
    "interface-over-type-literal": false,
    "no-console": false
  }
}
